getColor <- function(quakes) {
sapply(quakes$mag, function(mag) {
if (mag < 4) {
"green"
} else if (mag < 5) {
"black"
} else if (mag < 6) {
"blue"
} else if (mag < 7) {
"pink"
} else if (mag < 8) {
"orange"
} else {
"red"
} })
}
icons <- awesomeIcons(
icon = 'ios-close',
iconColor = 'black',
library = 'ion',
markerColor = getColor(df)
)
